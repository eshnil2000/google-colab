    "#Start Predictions\n",
    "with tf.Session() as sess:\n",
     "sess.run(init)\n",
     "predictions = pred.eval(feed_dict = {X:train_X})\n",
    
    "plt.plot(train_X, train_Y, 'ro', label='Original data')\n",
    
    "plt.plot(train_X, predictions, 'bx' ,label='Predicted data')\n",
    
    "plt.legend()\n",
    "plt.show()\n",
   ]
  }
